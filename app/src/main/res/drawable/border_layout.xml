<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:start="2dp"
        android:end="2dp"
        android:top="2dp"
        android:bottom="2dp">
        <shape android:shape="rectangle"> <!--사각형 모양으로 테두리 -->
            <stroke android:width="2dp" <!--테두리 두께 -->
            android:color="#ffffff"> <!--테두리 색 설정 -->
            <solid android:color="#ffffff"/> <!--내부색 설정 -->
        </shape>
    </item>
</selector>